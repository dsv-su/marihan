{
    "defaults": {
        "kurs1":{
            "path": "kurs1/"
        },
        "module_contrib": {
            "path": "sites/all/modules/contrib/"
        },
        "module_custom": {
            "path": "sites/all/modules/custom/"
        },
        "theme_contrib": {
            "path": "sites/all/themes/contrib/"
        },
        "theme_custom": {
            "path": "sites/all/themes/custom/"
        },
        "library": {
            "path": "sites/all/libraries/"
        }
    },

    "prim_landing": {
        "rootDir": "prim",
        "components": {
            "drupal_core": {
                "repo": "https://github.com/drupal/drupal.git",
                "path":"/var/www/prim"
            }
        }
    },

    "drupal": {
        "rootDir": "drupal",
        "requires": [ "prim_landing" ],
        "components": {
            "drupal_core": {
                "repo": "https://github.com/drupal/drupal.git",
                "branch": "7.x",
                "tag": "7.30",
                "patch": "core.patch",
                "ignoreEmptyPath": true
            }
        }
    },

    "prim_base": {
        "rootDir": "dsv_base",
        "requires": [ "drupal" ],
        "components": {
            "ctools": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/ctools.git",
                "branch": "7.x-1.x",
                "tag": "7.x-1.4",
                "path": "ctools"
            },
            "git_deploy": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/git_deploy.git",
                "branch": "7.x-2.x",
                "path": "git_deploy"
            },
            "date": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/date.git",
                "branch": "7.x-2.x",
                "tag": "7.x-2.8",
                "path": "date"
            },
            "entity": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/entity.git",
                "branch": "7.x-1.x",
                "tag": "7.x-1.5",
                "path": "entity"
            },
            "libraries": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/libraries.git",
                "branch": "7.x-2.x",
                "tag": "7.x-2.2",
                "path": "libraries"
            },
            "pathauto": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/pathauto.git",
                "branch": "7.x-1.x",
                "tag": "7.x-1.2",
                "path": "pathauto"
            },
            "rules": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/rules.git",
                "branch": "7.x-2.x",
                "tag": "7.x-2.7",
                "path": "rules"
            },
            "token": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/token.git",
                "branch": "7.x-1.x",
                "tag": "7.x-1.5",
                "path": "token"
            },
            "views": {
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/views.git",
                "branch": "7.x-3.x",
                "tag": "7.x-3.8",
                "path": "views",
                "patch": "views.patch"
            },
            "node_clone":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/node_clone",
                "branch": "7.x-1.x",
                "tag": "7.x-1.0-rc2",
                "path": "node_clone"
            },
            "nodeaccess":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/nodeaccess",
                "branch": "7.x-1.x",
                "tag": "7.x-1.3",
                "path": "nodeaccess"
            },
            "options_element":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/options_element",
                "branch": "7.x-1.x",
                "tag": "7.x-1.10",
                "path": "options_element"
            },
            "print":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/print",
                "branch": "7.x-1.x",
                "tag": "7.x-1.2",
                "path": "print"
            },
            "references":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/references",
                "branch": "7.x-2.x",
                "tag": "7.x-2.1",
                "path": "references"
            },
            "uuid":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/uuid",
                "branch": "7.x-1.x",
                "tag": "7.x-1.0-alpha5",
                "path": "uuid"
            },
            "webform":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/webform",
                "branch": "7.x-3.x",
                "tag": "7.x-3.7",
                "path": "webform"
            },
            "webform_conditional":{
                "defaults": "module_contrib",
                "repo": "git://git.drupal.org/project/webform_conditional",
                "branch": "7.x-1.x",
                "tag": "7.x-1.0-beta2",
                "path": "webform_conditional"
            }
        }
    },

    "prim_custom": {
        "components": {
            "prim_options":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/prim_options",
                "path": "prim_options"
            },
            "prim_validate":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/prim_validate",
                "path": "prim_validate"
            },
            "prim_form":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/prim_form",
                "path": "prim_form"
            },
            "prim_teacher_result":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/prim_teacher_result",
                "path": "prim_teacher_result"
            },
            "prim_tweeks":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/prim_tweeks",
                "path": "prim_tweeks"
            },
            "remove_menu_path":{
                "defaults": "module_custom",
                "repo": "https://github.com/dsv-su/remove_menu_path",
                "path": "remove_menu_path"
            }
        }
    },

    "prim_theme": {
        "components": {
            "mothership": {
                "defaults": "theme_contrib",
                "repo": "git://git.drupal.org/project/mothership.git",
                "branch": "7.x-2.x",
                "tag": "7.x-2.10",
                "path": "mothership"
            },
            "dsv_theme": {
                "defaults": "theme_custom",
                "repo": "https://github.com/dsv-su/dsv_theme.git",
                "path": "dsv_theme"
            },
            "su_prim_theme": {
                "defaults": "theme_custom",
                "repo": "https://github.com/dsv-su/su_prim_theme.git",
                "path": "su_prim_theme"
            }
        }
    },

    "kurs1": {
        "rootDir": "/var/www/prim/kurs1",
        "requires": [ "prim_base", "prim_custom", "prim_theme"  ],
        "components": {}
    },

    "ap3": {
        "rootDir": "/var/www/prim/ap3",
        "requires": [ "prim_base", "prim_custom", "prim_theme" ],
        "components": {}
    },

    "ap6": {
        "rootDir": "/var/www/prim/ap6",
        "requires": [ "prim_base", "prim_custom", "prim_theme" ],
        "components": {}
    },

    "ap9": {
        "rootDir": "/var/www/prim/ap9",
        "requires": [ "prim_base", "prim_custom", "prim_theme"  ],
        "components": {}
    },

    "uim": {
        "rootDir": "/var/www/prim/uim",
        "requires": [ "prim_base", "prim_custom", "prim_theme" ],
        "components": {}
    },

    "prim":{
        "rootDir": "/var/www/prim",
        "requires": [ "prim_landing", "kurs1", "ap3", "ap6", "ap9", "uim"  ],
        "components": {}
    }
}